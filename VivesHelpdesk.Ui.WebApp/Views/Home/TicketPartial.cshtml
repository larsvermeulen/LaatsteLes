@model Ticket

<article>
    <header>
        <h2>@Model.Title</h2>
    </header>
    <p>@Model.Description</p>
    <footer>
        Reported by @Model.Author on 
        @Model.CreatedDate.ToLocalTime().ToShortDateString() 
        at @Model.CreatedDate.ToLocalTime().ToShortTimeString().
    </footer>
</article>